{
  "name": "covid-backend-homelane",
  "version": "1.0.0",
  "description": "COVID-19 microservice for Homelane",
  "main": "index.js",
  "scripts": {
    "start": "node build/src/handlers/api.js",
    "postinstall": "rm -rf build && tsc && copyfiles src/data/*.csv build/src/data",
    "dev": "rm -rf db_covid && nodemon",
    "inspect": "rm -rf db_covid && nodemon --inspect src/handlers/api.ts",
    "test": "rm -rf db_covid && jest --runInBand",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .js,.ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "author": "Brijesh Sahoo",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.172",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.6",
    "supertest": "^6.1.5",
    "ts-jest": "^27.0.4"
  },
  "dependencies": {
    "@nano-sql/core": "^2.3.7",
    "axios": "^0.21.1",
    "celebrate": "^15.0.0",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.12",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": "14.x"
  }
}
